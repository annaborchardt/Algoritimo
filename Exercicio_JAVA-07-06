import java.util.Scanner;
public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int numero;
    System.out.print("Digite um n√∫mero: ");
    numero = sc.nextInt();
    System.out.println("/n");
    for (int i= 0 ; i<=10; i++) {
      System.out.println(numero + " x " + i + " = " + numero*i);




      
    }

    
  }

  
}
